{
  "name": "zlibt",
  "version": "0.1.0",
  "description": "typescript zlib",
  "main": "dist/zlibt.js",
  "bin": "bin/bin.js",
  "types": "zlib_and_gzip.min.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch src --delay 1 --exec ts-node src/zlibtest.ts",
    "build:prod": "webpack --config config/prod/config.js & webpack --config config/prod/config.umd.js",
    "build:prod:umd": "webpack --config config/prod/config.umd.js",
    "build:gzip:prod": "webpack --config config/prod/config.gzip.js",
    "build:zip:prod": "webpack --config config/prod/config.zip.js",
    "build:zlib:prod": "webpack --config config/prod/config.zlib.js",
    "build:raw:prod": "webpack --config config/prod/config.raw.js",
    "build:dev": "webpack --config config/dev/dev.config.js --watch",
    "build:gzip:dev": "webpack --config config/dev/dev.config.gzip.js --watch",
    "build:zip:dev": "webpack --config config/dev/dev.config.zip.js --watch",
    "build:zlib:dev": "webpack --config config/dev/dev.config.zlib.js --watch",
    "build:raw:dev": "webpack --config config/dev/dev.config.raw.js --watch",
    "predeploy:prod": "node ./deploy/zlibt.js",
    "lint": "tslint --project tsconfig.json src/*",
    "lint:fix": "tslint --project tsconfig.json src/* --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadfishDT/zlibt.git"
  },
  "keywords": [
    "zlib",
    "gzip",
    "zip",
    "js",
    "typescript"
  ],
  "author": "MadfishDT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MadfishDT/zlibt/issues"
  },
  "homepage": "https://madfishdt.github.io",
  "devDependencies": {
    "codelyzer": "~4.5.0",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^5.15.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.12",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {}
}
